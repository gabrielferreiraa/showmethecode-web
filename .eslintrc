{
  "extends": [
    "react-app",
    "plugin:sonarjs/recommended",
    "airbnb",
    "prettier"
  ],
  "plugins": ["sonarjs", "prettier"],
  "rules": {
    "prettier/prettier": ["error"]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "moduleDirectory": [
          "node_modules",
          "src"
        ]
      }
    }
  }
}
